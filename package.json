{
  "name": "vue3-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,types}/**/*.{vue,js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"{src,types}/**/*.{vue,js,jsx,ts,tsx,json,css,less,scss,html}\"",
    "lint": "pnpm lint:eslint && pnpm lint:prettier"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^9.5.0",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.5.0",
    "prettier": "^3.3.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.13.0",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
}
